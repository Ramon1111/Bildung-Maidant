<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/fmiemTWAdminCorreo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fmiemTWtitleadministrador"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="62dp"
        android:layout_marginLeft="62dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="86dp"
        android:layout_marginRight="86dp"
        style="@style/NormalTextView"
        android:paddingLeft="10dp"

        android:textColor="@color/secondary_text"
        android:textSize="17dp" />

    <ImageView
        android:id="@+id/fmiemICNuser"
        android:layout_width="73dp"

        android:layout_height="76dp"
        android:layout_alignTop="@+id/fmiemBTNagregarmiembro"
        android:layout_alignBottom="@+id/fmiemBTNagregarmiembro"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="-18dp"
        android:layout_marginEnd="279dp"
        android:layout_marginRight="279dp"
        android:layout_marginBottom="-11dp"
        app:srcCompat="@drawable/ic_action_user" />

    <TextView
        android:id="@+id/fmiemTWmiembros"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_gravity="center"
        android:layout_marginStart="133dp"
        android:layout_marginLeft="133dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="97dp"
        android:layout_marginRight="97dp"
        android:fontFamily="sans-serif-condensed-light"
        android:gravity="center"
        android:text="@string/Miembros"
        android:textColor="@color/primary"
        android:textSize="35dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/fmiemICNusers"
        android:layout_width="73dp"
        android:layout_height="76dp"

        android:layout_alignTop="@+id/fmiemTWmiembros"
        android:layout_alignBottom="@+id/fmiemTWmiembros"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="-15dp"
        android:layout_marginEnd="295dp"
        android:layout_marginRight="295dp"
        android:layout_marginBottom="-14dp"
        app:srcCompat="@drawable/ic_action_users" />

    <Button
        android:id="@+id/fmiemBTNagregarmiembro"
        android:layout_width="189dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fmLVMiembros"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="122dp"
        android:layout_marginLeft="122dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="99dp"
        android:layout_marginRight="99dp"
        android:background="@color/accent"
        android:fontFamily="sans-serif-condensed-light"
        android:text="@string/Agregar_miembro"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/fmiemTWlistadminis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fmiemTWtitleadministrador"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="62dp"
        android:layout_marginLeft="62dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="86dp"
        android:layout_marginRight="86dp"
        android:fontFamily="sans-serif-condensed-light"

        android:textColor="@color/primary_text"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/fmiemTWtitleadministrador"
        android:layout_width="196dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fmiemTWmiembros"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="155dp"
        android:layout_marginRight="155dp"
        android:text="@string/Administrador"
        android:fontFamily="sans-serif-condensed-light"
        android:textColor="@color/primary_dark"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/fmiemTWtitlemiembros"
        android:layout_width="114dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fmiemTWAdminCorreo"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="229dp"
        android:layout_marginRight="229dp"
        android:fontFamily="sans-serif-condensed-light"
        android:text="@string/Usuarios"
        android:textColor="@color/primary_dark"
        android:textSize="25dp" />

    <ListView
        android:id="@+id/fmLVMiembros"
        android:layout_width="300dp"
        android:layout_height="260dp"
        android:layout_below="@+id/fmiemTWtitlemiembros"
        android:layout_alignStart="@+id/fmiemTWlistadminis"
        android:layout_alignLeft="@+id/fmiemTWlistadminis"
        android:layout_marginStart="-1dp"
        android:layout_marginLeft="-1dp"
        android:layout_marginTop="-1dp"
        android:scrollbarAlwaysDrawVerticalTrack="true"
        android:textSize="20dp" />


</RelativeLayout>
